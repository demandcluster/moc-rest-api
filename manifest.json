{"endpoints": {
	"/items/:shopId/:language/:language1/:search": {  
		"get": {     
			"operation": "catalogItems",
			"params":{
				"shopId":"shopId",
				"searchQuery": "search",
				"language": "language",
				"language1": "language1"
			}
		}
	},
	"/buckaroo":{
		"post": {
			"operation":"setBuckarooCallbackData",
		    "wrapRequestBodyWith":"input",
			"successStatusCode": 201
		}
	}
  }
}
